set -x

if [ -f "/usr/share/anon-gw-base-files/gateway" ]; then
   kdesudo kwrite /etc/whonix_firewall.d/30_whonix_gateway_default.conf
else
   kdesudo kwrite /etc/whonix_firewall.d/30_whonix_workstation_default.conf
fi
